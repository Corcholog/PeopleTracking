{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "my-capability",
  "description": "Allow dialog functionality",
  "windows": ["*"],
    "permissions": [
    "core:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:default",
    { "identifier": "fs:read-all", "allow": [{ "path": "$DOWNLOAD/**" }] }
  ]
}
