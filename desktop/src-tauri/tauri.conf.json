{
  "identifier": "com.agustin.peopletracking",
  "productName": "PeopleTracking",
  "version": "1.0.0",
  "build": {
    "beforeDevCommand": "cd ../frontend && npm run dev",
    "beforeBuildCommand": "cd ../frontend && npm run build",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../../frontend/out"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "PeopleTracking",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "externalBin": ["binaries/fastapi_server"],
    "targets": "all",
    "targets": ["msi", "appimage", "deb", "rpm"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/[email protected]",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
